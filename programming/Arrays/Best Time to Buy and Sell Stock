class Solution{
  public int maxProfit(int[]prices){
    int min_val=Integer.MAX_VALUE;
    int max_val=0;

    for(int i=o;i<prices.length;i++){
        if(prices[i]<min_val){
          min_val=prices[i];
        }

         else if(nums[i]-min_val>max_val){
          max_val=nums[i]-min_val;
         }
     }
   return max;
  }

}
